{
  "name": "github-qe-portfolio",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@apidevtools/swagger-parser": "12.1.0",
    "@axe-core/playwright": "4.11.0",
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@faker-js/faker": "10.2.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@playwright/test": "1.57.0",
    "@types/jest": "30.0.0",
    "@types/k6": "0.54.0",
    "@types/node": "25.0.9",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "axe-core": "4.11.1",
    "babel-loader": "9.2.1",
    "clean-webpack-plugin": "4.0.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-playwright": "2.5.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "glob": "13.0.0",
    "husky": "9.1.7",
    "istanbul-lib-report": "3.0.1",
    "jest": "30.2.0",
    "k6": "0.0.0",
    "lint-staged": "16.2.7",
    "msw": "2.12.7",
    "nyc": "15.1.0",
    "openapi-typescript": "7.10.1",
    "prettier": "3.8.0",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "webpack": "5.98.0",
    "webpack-cli": "5.1.4",
    "zod": "4.3.5"
  },
  "scripts": {
    "build": "echo \"No build step required; Playwright tests only\"",
    "test": "npx playwright test",
    "test:smoke": "npx playwright test tests/smoke",
    "test:regression": "npx playwright test tests --grep -v @smoke",
    "test:all": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:fast": "npx playwright test --grep @fast",
    "test:medium": "npx playwright test --grep @medium",
    "test:slow": "npx playwright test --grep @slow",
    "test:quick": "npx playwright test --grep '@fast|@medium'",
    "test:parallel": "npx playwright test --workers=4",
    "test:analyze": "npx ts-node scripts/optimize-execution.ts",
    "test:unit": "jest --passWithNoTests",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage --passWithNoTests",
    "test:coverage": "nyc --reporter=text-summary --reporter=html npm test",
    "coverage:report": "nyc report --reporter=html && open coverage/index.html",
    "coverage:check": "nyc check-coverage --lines 70 --functions 70 --branches 60 --statements 70",
    "unit:check": "jest --coverage --passWithNoTests --thresholdAlert",
    "perf:audit": "lhci autorun",
    "perf:local": "lighthouse https://automationexercise.com --view",
    "security:check": "./scripts/local-security-check.sh",
    "security:audit": "npm audit --audit-level=high",
    "visual:run": "npx playwright test --grep @visual",
    "visual:run:chrome": "npx playwright test --grep @visual --project=chromium",
    "visual:run:all-browsers": "npx playwright test --grep @visual --project=chromium --project=firefox --project=webkit",
    "visual:update": "npx playwright test --grep @visual --update-snapshots",
    "visual:update:chrome": "npx playwright test --grep @visual --project=chromium --update-snapshots",
    "visual:update:all": "npx playwright test --grep @visual --project=chromium --project=firefox --project=webkit --update-snapshots",
    "visual:reset": "npm run visual:update -- --force",
    "visual:report": "npx playwright show-report",
    "visual:detect-flakes": "npx ts-node scripts/detect-flakes.ts --runs 5",
    "visual:detect-flakes:quick": "npx ts-node scripts/detect-flakes.ts --runs 3",
    "visual:detect-flakes:chrome": "npx ts-node scripts/detect-flakes.ts --runs 5 --project chromium",
    "load:api": "k6 run tests/load/compiled/products-load.test.js",
    "load:e2e": "k6 run tests/load/compiled/user-journey.test.js",
    "load:smoke": "k6 run --vus 1 --duration 10s tests/load/compiled/products-load.test.js",
    "load:stress": "k6 run --vus 100 --duration 5m tests/load/compiled/products-load.test.js",
    "load:spike": "k6 run --stage 10s:0,1m:100,10s:0 tests/load/compiled/products-load.test.js",
    "load:build": "webpack --config tests/load/webpack.config.js",
    "load:build:watch": "webpack --config tests/load/webpack.config.js --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
